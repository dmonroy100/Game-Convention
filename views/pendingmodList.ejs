
    <ol>
    <% for(i = 0; i< modLists.length; i++){  %>
      <% if (modLists[i].m_level==0) { %>
          <li>
                Name: <%= modLists[i].googlename%> <br>
                UserEmail:  <%= modLists[i].googleemail %>    <br>
                Reasons: <%= modLists[i].m_reason %> <br>
                SubmitAt: <%= modLists[i].m_createdAt.toLocaleDateString('en-US') %> <br>
                Level:  <%= modLists[i].m_level %> <br>

                <form method="post" action="/processLevel0" >
                    <input type="hidden" name="modid" value="<%= modLists[i]._id %>" >
                    <input type="hidden" name="convid" value="<%= modLists[i].m_convId %>" >
                <button type="submit" name="deny">Denied</button>
                </form>
                <form method="post" action="/processLevel1" >
                    <input type="hidden" name="modid" value="<%= modLists[i]._id %>" >
                    <input type="hidden" name="convid" value="<%= modLists[i].m_convId %>" >
                <button type="submit" name="approval">Level 1</button>
                </form>
                <form method="post" action="/processLevel2/<%= convention._id %>" >
                    <input type="hidden" name="modid" value="<%= modLists[i]._id %>" >
                    <input type="hidden" name="convid" value="<%= modLists[i].m_convId %>" >
                <button type="submit" name="approval">Level 2</button>
                </form>
                <form method="post" action="/processLevel3/<%= convention._id %>" >
                    <input type="hidden" name="modid" value="<%= modLists[i]._id %>" >
                    <input type="hidden" name="convid" value="<%= modLists[i].m_convId %>" >
                <button type="submit" name="approval">Level 3</button>
                </form>
                <form method="post" action="/processLevel4/<%= convention._id %>" >
                    <input type="hidden" name="modid" value="<%= modLists[i]._id %>" >
                    <input type="hidden" name="convid" value="<%= modLists[i].m_convId %>" >
                <button type="submit" name="approval">Level 4</button>
                </form>
                <form method="post" action="/processLevel5/<%= convention._id %>" >
                    <input type="hidden" name="modid" value="<%= modLists[i]._id %>" >
                    <input type="hidden" name="convid" value="<%= modLists[i].m_convId %>" >
                <button type="submit" name="approval">Level 5</button>
                </form>
          </li>
          <% } %>
      <% } %>
      </ol>
